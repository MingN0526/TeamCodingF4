@model TeamCodingF4.Models.EstateModel

<div class="container-fluid">
    <div class="row px-xl-5">
        <div class="col-12">
            <nav class="breadcrumb bg-light mb-30">
                <a class="breadcrumb-item text-dark" href="#">首頁</a>
                <a class="breadcrumb-item text-dark" href="/estate/index">我的物件</a>
                <span class="breadcrumb-item active">確定刪除</span>
            </nav>
        </div>
    </div>
</div>



<div>
    <dl class="row">

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EstateTittle)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RoomType)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EststePrice)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Miscellaneous)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Meters)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Car)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Motorcycle)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AirConditioner)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Television)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WetDry)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Balcony)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WashingMachine)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WaterDispenser)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Refrigerator)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Lease)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EstateImage)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EstateVideo)
        </dd>

        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.message)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="EstateId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
    </form>
</div>
