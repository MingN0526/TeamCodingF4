    {
    <script>
        let app = new Vue({
            el: "#estateIndex",
            data: {
                inf:[]
            },
            mounted: {
                funtion(){
                    let self=this;
                    fetch("/api/EstateApi/Index")
                    .then(function(res){self.inf=res.data})
                }
            },
