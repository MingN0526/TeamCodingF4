                                <i class="fa fa-times">
                                    <a action="Estate/Edit"></a>
                                </i>
                            </button>
                            <button class="btn btn-sm btn-danger">
                                <i class="fa fa-times">
                                    <a action="Estate/Create"></a>
                                </i>
                            </button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>

