                                al.icon = "error";
                                al.title = "發佈失敗";
                            }
                            Swal.fire(al)
                            window.location = "/article/index"
                        })
                },
                filterArticles: function () {
                    var _self = this
                    var temp = this.articles
                    if (this.Filter.length > 0) {
