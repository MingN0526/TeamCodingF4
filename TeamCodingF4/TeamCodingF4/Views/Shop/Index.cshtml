                        })
                       fetch("/api/equipment/GetAllHouseEquipment")
                         .then(function (response) { return response.json() })
                         .then(function (result) {
                             console.log(result)
                             _self.houseEquipment = result
                        })
                },
                methods: {
                    showAllRoom:function(){
                        let _self = this
                        fetch("/api/house/GetAllHouse")
                            .then(function (response) { return response.json() })
                            .then(function (result) {
                                console.log(result)
                                _self.house = result
                                _self.homeRoomType = result
                                _self.allHouse = result
                            })                        
                    },
                    filterRoomType:function(roomType){
                        var temp = []
                        for (let i = 0; i <= 4; i++){
                                if (this.house[i].roomType.name == roomType) {
                                    temp.push(this.house[i])
                            }
                        }
                        this.homeRoomType = temp
                        this.allHouse = this.homeRoomType
                    },
                    filterRoomEquipment:function(equipment){
                        var temp = []
                        for (let i = 0; i <= 7; i++){
                            if (this.house[i].equipment.name == equipment) {
                                temp.push(this.house[i])
                            }
                        }
                        this.equipment = temp
                        this.allHouse = this.equipment
                    },
                    filterTransportation:function(transportation){
                        var temp = []
                        for (let i = 0; i <= 4; i++){
                            if (this.house[i].roomType.name == transportation) {
                                 temp.push(this.house[i])
                            }
                        }
                        this.homeRoomType = temp
                        this.allHouse = this.homeRoomType
                    },
                    filterRoomType:function(roomType){
                        var temp = []
                        for (let i = 0; i <= 4; i++){
                                if (this.house[i].roomType.name == roomType) {
                                    temp.push(this.house[i])
                            }
                        }
                        this.homeRoomType = temp
                        this.allHouse = this.homeRoomType
                    },
                    filterHouse: function () {                      
                            var temp = this.house
                        if (this.Filter.length > 0) {
                            temp = temp.filter(function (temp) {
                                  return temp.tittle.includes(this.Filter)
                            })
                        }
                        this.FilterBox = temp
                        this.allHouse = this.FilterBox
                    },
                },
            });
