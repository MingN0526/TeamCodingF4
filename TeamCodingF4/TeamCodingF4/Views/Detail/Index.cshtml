                        //console.log(this.articles[i])
                        if (this.articles[i].category == type) {
                            temp.push(this.articles[i])
                        }
                    }
                    this.show = temp;
                },
                insertArticle: function (addArticle) {
                    addArticle = this.addArticle
                    fetch("/api/article/postarticle", { 
                        method: "POST" ,
                        body: JSON.stringify(addArticle),
                        headers:{"content-type":"application/json"}
                    }).then(function(response) {return response.json()})
